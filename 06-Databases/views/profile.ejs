<!-- views/profile.ejs -->
<!doctype html>
<html>

<%- include partials/header.ejs %>

<body>
	<%- include partials/navbar.ejs %>

	<section id="my_panel" class="row user-panel">
		<div class="col-md-10 col-center">
			<div class="row">
				<div class="col-md-3">
				</div>
				<div class="col-md-6">
					<h2 style="font-size: 24px; color:  #5d5d5d;">User Panel</h2>
				</div>
			</div>

			<div class="row">
				<div class="col-md-4 "></div>
				<div class="col-md-4 ">
					<ul>
						<%  Object.keys(user).forEach(function (key) { %>
						<li> <strong>
								<%= key %></strong><span style="float: right">
								<%= user[key] %></span></li>
						<% }) %>
					</ul>

				</div>
	</section>
</body>

</html>